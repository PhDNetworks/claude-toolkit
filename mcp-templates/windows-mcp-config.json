{
  "mcpServers": {
    "asana": {
      "type": "stdio",
      "command": "cmd",
      "args": ["/c", "npx", "-y", "@roychri/mcp-server-asana"],
      "env": {
        "ASANA_ACCESS_TOKEN": "YOUR_ASANA_PAT"
      }
    },
    "google-calendar": {
      "type": "stdio",
      "command": "cmd",
      "args": ["/c", "npx", "-y", "@cocal/google-calendar-mcp"],
      "env": {
        "GOOGLE_OAUTH_CREDENTIALS": "%USERPROFILE%\\Documents\\credentials\\google-calendar-client-secret.json"
      }
    },
    "notion": {
      "type": "stdio",
      "command": "cmd",
      "args": ["/c", "npx", "-y", "mcp-remote", "https://mcp.notion.com/mcp"],
      "env": {}
    },
    "make": {
      "type": "stdio",
      "command": "cmd",
      "args": ["/c", "npx", "-y", "mcp-remote", "https://YOUR_REGION.make.com/mcp/u/YOUR_ORG_ID/sse"],
      "env": {}
    },
    "playwright": {
      "type": "stdio",
      "command": "cmd",
      "args": ["/c", "npx", "@playwright/mcp@latest"],
      "env": {}
    }
  }
}
